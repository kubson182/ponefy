<?xml version='1.0' encoding='utf-8'?>
<widget id="com.codesundar.ponefy" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>P-N-F</name>
    <description>
       App to track positive, negative and FYI events.
    </description>
    <author email="j.krchlik@gmail.com" href="http://phonegap.com">
        Jakub Krchl√≠k
    </author>
    <content src="index.html" />
    <access origin="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
    <access origin="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
